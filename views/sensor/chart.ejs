      <div class="row">
        <div class="col-lg-12">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">roomArea1</h5>

              <div class="row">
                <div class="col-lg-6">

              <!-- Line Chart -->
              <div id="lineChart1" style="min-height: 400px;" class="echart"></div>

              <script>
                document.addEventListener("DOMContentLoaded", () => {
                  echarts.init(document.querySelector("#lineChart1")).setOption({
                    title: {
                        text: 'Temperature  '

                    },
                    xAxis: {
                      type: 'category',
                      label: 'hari',
                      data : ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'],
                    },
                    yAxis: {
                      type: 'value'
                    },
                    legend: {
                      top: '5%',
                      left: 'center'
                    },
                    series: [
                    {
                      name: 'Max',
                      data: [<%= datatempmax1%>],
                      type: 'line',
                      smooth: true,
                      lineStyle: {color: '#ff0000'}
                    },
                    {
                      name: 'Min',
                      data: [<%= datatempmin1%>],
                      type: 'line',
                      smooth: true,
                      lineStyle: {color: '#0000ff'}
                    },
                    {
                      name: 'Avg',
                      data: [<%= datatempavg1%>],
                      type: 'line',
                      smooth: true,
                      lineStyle: {color: '#39ac39'}
                    }
                ]
                  });
                });
              </script>
              <!-- End Line Chart -->
              <!-- Line Chart -->

              </div>
